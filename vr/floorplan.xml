<plugin name="activespot" url="floorplan/hotspot-floorplan-active.png" keep="true" scale="0.8" visible="false" align="center" zorder="3" edge="center"   /> 

     <!-- activated hotpot -->

    <action name="activatespot">
        set(plugin[activespot].parent, plugin[%1]);
        set(plugin[activespot].visible, true);
        
        copy(plugin[radar].x, plugin[%1].x);
        copy(plugin[radar].y, plugin[%1].y);
        
        set(plugin[radar].visible, true);
        set(plugin[radar].heading, %2);
    </action>
    
    <plugin name="radar"
            url="%SWFPATH%/plugins/radar.swf"
            alturl="%SWFPATH%/plugins/radar.js"
            zorder="1"
            keep="true"
            heading="0"
            align="lefttop"
            edge="center"
            x="0" y="0"
            fillcolor="0x3b3b3b"
            scale="0.4"
            visible="true"  
            editmode="1" 
            />

<layer name="floorplan_button"  url="images/infobutton_ecard.png" crop="0|74|200|37"  keep="true" zorder="50"   align="centerright"  X="-152"  Y="37"  
	onover="tween(layer[floorplan_button].x,0);"  onout="tween(layer[floorplan_button].x,-152);" onclick="showfloorplan()"    />

